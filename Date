#include <iostream>
using namespace std;

int main() 
{
    int day;
    int month;
    int year;
    
    cout<< "PLEASE PUT NUMBERS ONLY\n";
    
 
    cout << "Please enter the DAY:"; 
    cin >> day;
    
    cout << "Please enter the MONTH:";
    cin >> month;
    
    cout << "Please enter the YEAR:";
    cin >> year;

   if ( day > 31 )
    {
        cout << "THE DATE IS INVALID "  << endl;
    }
   
    else if ( month == 1)
    {
        cout << "January " << day << "," << year << endl;
    }
    
    else if ( month ==2 )
    {
        cout << "February " << day << "," << year << endl;
    }
    else if ( month ==3 )
    {
        cout << "March " << day << "," << year << endl;
    }
    else if ( month ==4 )
    {
        cout << "April " << day << "," << year << endl;
    }
    else if ( month ==5 )
    {
        cout << "May " << day << "," << month << endl;
    }
    else if ( month ==6 )
    {
        cout << "June " << day << "," << month << endl;
    }
    else if ( month ==7 )
    {
        cout << "July " << day << "," << month << endl;
    }
    else if ( month ==8 )
    {
        cout << "August " << day << "," << month << endl;
    }
    else if ( month ==9 )
    {
        cout << "September " << day << "," << month << endl;
    }
    else if ( month ==10 )
    {
        cout << "October " << day << "," << month << endl;
    }
    else if ( month ==11 )
    {
        cout << "November " << day << "," << month << endl;
    }
    else if ( month ==12 )
    {
        cout << "December " << day << "," << month << endl;
    }
    
    
    
    else if ( month > 12 )
    {
        cout << "INVALID DATE!"  << endl;
    }
   

    
    return 0;
}
